{{!
    @template local_vetagropro/event_log

    This template will render a list of event log

}}

<div>
        <table class="generaltable fullwidth">
            <thead>
            <tr>
                {{#columns}}
                    <th scope="col">{{#str}}{{.}}, local_vetagropro{{/str}}</th>
                {{/columns}}
            </tr>
            </thead>
            <tbody>
            {{#userdatalog}}
                <tr>
                    {{#values}}
                        <td>{{.}}</td>
                    {{/values}}
                </tr>
            {{/userdatalog}}
            </tbody>
        </table>
</div>